{
  "name": "accordium-react-ui-toolkit",
  "description": "UI Component Toolkit for Accordium Apps",
  "version": "0.1.6",
  "homepage": "https://www.accordium.com",
  "author": "Accordium Developer",
  "license": "MIT",
  "scripts": {
    "build:ultra": "rm -rf dist && sass src/ultra/lib/style/accordium-ui-toolkit.scss:src/ultra/lib/style/accordium-ui-toolkit.css --style compressed && NODE_ENV=production babel src/ultra/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__ --no-comments",
    "storybook-build:ultra": "build-storybook -c storybook -o .out",
    "storybook:ultra": "start-storybook -p 3002 -c storybook --debug-webpack",
    "compile:cssonly": "NODE_ENV=production LIBRARY_TARGET=cssonly webpack --config webpack.config.babel.js --require @babel/register --mode production && del titan/cssonly/*.js",
    "compile:commonjs": "NODE_ENV=production LIBRARY_TARGET=commonjs webpack --config webpack.config.babel.js --require @babel/register --mode production",
    "compile:umd": "NODE_ENV=production LIBRARY_TARGET=umd webpack --config webpack.config.babel.js --require @babel/register --mode production",
    "transpile": "NODE_ENV=production babel src/titan --out-dir titan --source-maps inline --copy-files --ignore stories.js,__tests__,spec.js,test.js,__snapshots__ --no-comments && del titan/**/*.test.js titan/**/*.spec.js titan/**/*.stories.js ",
    "build": "rm -rf titan && yarn compile:commonjs && yarn compile:umd && yarn transpile",
    "deploy": "build-storybook -c storybookTitan -o .out && now --prod",
    "storybook": "start-storybook -p 3005 -c storybookTitan --debug-webpack",
    "sass-lint:fix": "sass-lint-auto-fix 'src/**/*.scss' --verbose --no-exit"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash.debounce": "^4.0.8",
    "lodash.uniqueid": "^4.0.1",
    "lottie-web": "^5.5.9",
    "ramda": "^0.26.1",
    "rc-slider": "^8.7.1",
    "react-debounce-input": "^3.2.2",
    "react-is": "^16.11.0",
    "react-player": "^1.13.0",
    "react-spring": "^8.0.27",
    "react-textarea-autosize": "^7.1.2",
    "screenfull": "^5.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
