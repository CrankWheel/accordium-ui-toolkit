    "sass-lint-auto-fix": "^0.17.0",
    "sass-loader": "^7.1.0",
    "static-auth": "^1.1.1",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
